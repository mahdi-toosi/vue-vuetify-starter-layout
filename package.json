{
	"$schema": "https://json.schemastore.org/package.json",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"build": "cross-env NODE_ENV=production run-p type-check build-only",
		"build:analyze": "vite build --mode analyze",
		"clean": "rimraf ./node_modules/.vite && rimraf dist",
		"preview": "vite preview",
		"merge": "node scripts/create-merge-request.js",
		"lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
		"lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
		"format:check": "prettier .",
		"format:fix": "prettier -u --write .",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"prepare": "husky install",
		"generate:changelog": "standard-version --skip.commit --skip.tag",
		"release": "standard-version --bumpFiles './manifest.json' --packageFiles './manifest.json' && git push origin main --follow-tags"
	},
	"dependencies": {
		"@mdi/font": "^7.1.96",
		"axios": "^1.3.4",
		"pinia": "^2.0.32",
		"pinia-plugin-persistedstate": "^3.1.0",
		"remove": "^0.1.5",
		"vue": "^3.2.47",
		"vue-router": "^4.1.6",
		"vuetify": "^3.1.6"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@types/jsdom": "^21.1.0",
		"@types/node": "^18.14.1",
		"@typescript-eslint/parser": "^5.53.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitest/coverage-c8": "^0.29.1",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.3.0",
		"@vue/tsconfig": "^0.1.3",
		"autoprefixer": "^10.4.13",
		"cross-env": "^7.0.3",
		"eslint": "^8.34.0",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.9.0",
		"eslint-plugin-vuetify": "^2.0.0-beta.2",
		"husky": "^8.0.3",
		"jsdom": "^21.1.0",
		"lint-staged": "^13.1.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.21",
		"postcss-html": "^1.5.0",
		"postcss-scss": "^4.0.6",
		"prettier": "^2.8.4",
		"rimraf": "^4.1.2",
		"rollup-plugin-analyzer": "^4.0.0",
		"rollup-plugin-visualizer": "^5.9.0",
		"sass": "^1.58.3",
		"standard-version": "^9.5.0",
		"stylelint": "^15.2.0",
		"stylelint-config-recommended-scss": "^9.0.1",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-order": "^6.0.2",
		"tailwindcss": "^3.2.7",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-vuetify": "^1.0.2",
		"vitest": "^0.29.1",
		"vue-eslint-parser": "^9.1.0",
		"vue-tsc": "^1.2.0"
	},
	"resolutions": {
		"json5": "^2.2.3",
		"http-cache-semantics": "^4.1.1"
	}
}
